generator client {
  provider = "prisma-client-js"
  previewFeatures = ["jsonProtocol"]
}

// 연결 정보
datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL")
}

model User {
  id  String  @default(cuid())  @id
  oauth_id  String
  nickname  String
  created_at  DateTime
  updated_at  DateTime
}